import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

file_path = "../data/Exp_3_VBOX_Data.xlsx"
sheets = ['7', '8', '9']
df = pd.concat([pd.read_excel(file_path, sheet_name=s) for s in sheets])
df.head()

df = df[['Speed (kmph)', 'Time']].dropna()
df.describe()

df['Time'] = df['Time'].astype(int)
avg_speed = df.groupby('Time')['Speed (kmph)'].mean()
avg_speed.head()

plt.figure(figsize=(9,5))
sns.lineplot(x=avg_speed.index, y=avg_speed.values)
plt.xlabel("Time")
plt.ylabel("Average Speed (kmph)")
plt.title("Traffic Mobility Trend Based on Speed")
plt.show()
